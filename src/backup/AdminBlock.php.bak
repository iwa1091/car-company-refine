<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Model;
use Illuminate\Database\Eloquent\Factories\HasFactory;

class AdminBlock extends Model
{
    use HasFactory;

    protected $fillable = [
        'date',
        'start_time',
        'end_time',
        'lane',
        'name',
        'maker',
        'car_model',
        'course',
        'menu',
        'notes',
    ];

    protected $casts = [
        'date' => 'date',
    ];
}
